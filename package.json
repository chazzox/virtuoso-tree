		"virtualisation",
		"virtualized",
		"react",
		"reactjs",
		"virtual",
		"endless",
		"tree",
		"infinite"
	],
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test --passWithNoTests",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"size": "size-limit",
		"analyze": "size-limit --why"
	},
	"peerDependencies": {
		"react": ">=16",
		"react-virtuoso": "^1.5.6"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"module": "dist/mylib.esm.js",
	"size-limit": [
		{
			"path": "dist/mylib.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/mylib.esm.js",
			"limit": "10 KB"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^4.9.2",
		"@types/js-cookie": "^2.2.6",
