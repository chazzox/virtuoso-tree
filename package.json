		"@types/react-window": "^1.8.2",
		"@types/react-dom": "^17.0.1",
		"husky": "^5.0.9",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-virtuoso": "^1.5.6",
		"size-limit": "^4.9.2",
		"tsdx": "^0.14.1",
		"tslib": "^2.1.0",
		"typescript": "^4.1.5"
	},
	"dependencies": {
		"react-merge-refs": "^1.1.0",
		"react-window": "^1.8.6"
