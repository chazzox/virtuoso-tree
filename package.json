{
	"name": "virtuoso-tree",
	"author": "chazzox",
	"homepage": "https://chazzox.github.io/virtuoso-tree/",
	"repository": {
		"type": "github",
		"url": "https://github.com/chazzox/virtuoso-tree"
	},
	"private": false,
	"version": "0.0.1",
	"license": "GPL v3.0",
	"main": "dist/index.js",
